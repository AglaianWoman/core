  "require": {
    "php": ">=5.5.9",
    "psr/http-message": "1.0",
    "zendframework/zend-diactoros": "^1.0"
  },

  "require-dev":{
    "phpunit/phpunit": "4.1.2",
    "squizlabs/php_codesniffer": "~2.5"
  }
