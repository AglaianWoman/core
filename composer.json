{

  "minimum-stability": "dev",
  "prefer-stable": true,

  "name": "gsouf/serps",
  "description": "Search engine scrapper",
  "type": "library",
  "keywords": ["google", "yahoo", "bing", "search engine", "scrapper"],
  "homepage": "https://github.com/gsouf/serpy",
  "license": "Fair",

  "authors": [
    {
      "name": "Soufiane GHZAL",
      "homepage": "https://github.com/gsouf"
    }
  ],

  "autoload":{
    "psr-4" : {
